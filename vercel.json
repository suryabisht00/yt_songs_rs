{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "app.py": {
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "env": {
        "PYTHONPATH": "/var/task",
        "TMPDIR": "/tmp"
    }
}